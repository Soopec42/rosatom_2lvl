#include <iostream>
#include <vector>
#include <utility>
#include <unordered_map>
#include <algorithm>

using namespace std;



int main() {
	
	long n;
	cin >> n;
	vector<long> v(n);
	vector<long> h(n);
	vector<long> vcou(n, -1);
	vector<long> hcou(n, -1);
	for (long i = 0; i < n; i++)
	{
		cin >> v[i];
	}

	for (long i = 0; i < n; i++)
	{
		cin >> h[i];
	}

	for (long i = n - 1; i >= 0; i--)
	{
		for (long j = 0; j < n; j++)
		{
			if (hcou[j] == -1)
			{
				if (v[i] - (n - i - 1) + j == h[j])
				{
					vcou[i] = n + j + 1;
					hcou[j] = i + 1;
					break;
				}
			}
			

		}
	}
	for (long i = 0; i < n; i++)
	{
		cout << vcou[i] << " ";
	}
	cout << endl;
	for (long i = 0; i < n; i++)
	{
		cout << hcou[i] << " ";
	}

}
